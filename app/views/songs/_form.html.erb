<%= form_for @song do |f| %>

    <ul>
    <% @song.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>

  <%= f.label :title %>
  <%= f.text_field :title %>



  <%= f.label :artist %>
  <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>
  <%= f.submit %>
<% end %>
